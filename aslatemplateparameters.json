{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "vmName": {
            "value": "[parameters('vmName')]"
        },
        "location": {
            "value": "[parameters('location')]"
        },
        "vmExtensionPublisherSecurity": {
            "value": "[variables('vmExtensionPublisherSecurity')]"
        },
        "vmExtensionTypeSecurity": {
            "value": "[variables('vmExtensionTypeSecurity')]"
        },
        "vmExtensionTypeHandlerVersionSecurity": {
            "value": "[variables('vmExtensionTypeHandlerVersionSecurity')]"
        }
    }
}